    "aws-cdk": "^2.1024.0",
    "aws-cdk-lib": "^2.210.0",
    "lucide-react": "^0.539.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@aws-amplify/backend-cli": "^1.8.0",
    "@eslint/js": "^9.32.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
