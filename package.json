  "version": "1.0.0",
  "description": "A cloud-native bucket list tracker application built with React and AWS Amplify",
  "type": "module",
  "homepage": "https://github.com/your-username/bucket-list-tracker",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/bucket-list-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/bucket-list-tracker/issues"
  },
  "keywords": [
    "react",
    "aws-amplify",
    "bucket-list",
    "cloud",
    "serverless",
    "graphql",
    "cognito",
    "s3",
    "dynamodb"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "echo \"No tests specified yet\" && exit 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "amplify:init": "amplify init",
    "amplify:push": "amplify push",
    "amplify:pull": "amplify pull",
    "amplify:status": "amplify status",
    "amplify:console": "amplify console",
    "ampx:configure": "npx ampx configure",
    "ampx:deploy": "npx ampx pipeline-deploy",
    "ampx:sandbox": "npx ampx sandbox",
    "deploy:staging": "amplify publish --environment staging",
    "deploy:production": "amplify publish --environment production"
  },
  "dependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/ui-react": "^6.11.2",
    "@aws-amplify/ui-react-storage": "^3.12.0",
