# Environment Configuration for Bucket List Tracker

# Application
VITE_APP_NAME=Bucket List Tracker
VITE_APP_VERSION=1.0.0

# AWS Configuration (Replace with your actual values)
VITE_AWS_REGION=us-east-1
VITE_AWS_USER_POOL_ID=us-east-1_XXXXXXXXX
VITE_AWS_USER_POOL_WEB_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_AWS_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# AWS AppSync GraphQL API
VITE_AWS_APPSYNC_GRAPHQL_ENDPOINT=https://xxxxxxxxxxxxxxxxxxxxxxxxxx.appsync-api.us-east-1.amazonaws.com/graphql
VITE_AWS_APPSYNC_REGION=us-east-1
VITE_AWS_APPSYNC_AUTHENTICATION_TYPE=AMAZON_COGNITO_USER_POOLS

# AWS S3 Storage
VITE_AWS_STORAGE_BUCKET=your-bucket-name
VITE_AWS_STORAGE_REGION=us-east-1

# Development
NODE_ENV=development
VITE_DEV_MODE=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Feature Flags (Optional)
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false
